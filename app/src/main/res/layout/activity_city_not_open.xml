<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/all_rl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backGround8">

    <LinearLayout
        android:id="@+id/item_ll"
        style="@style/tip_rl_style">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/dp_15"
            android:src="@mipmap/icon_big_weixin" />

        <com.mall.sls.common.widget.textview.ConventionalTextView
            style="@style/size12_color_app3_text_style"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/dp_32"
            android:layout_marginBottom="@dimen/dp_47"
            android:gravity="center_horizontal"
            android:text="@string/city_not_open" />
    </LinearLayout>
</RelativeLayout>