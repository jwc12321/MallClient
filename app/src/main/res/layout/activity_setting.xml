<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_rl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backGround6"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/title_rel"
        style="@style/title_rl_style">

        <ImageView
            android:id="@+id/back"
            style="@style/title_left_back_style" />

        <com.mall.sls.common.widget.textview.MediumThickTextView
            android:id="@+id/title"
            style="@style/title_text_style"
            android:text="@string/setting" />
    </RelativeLayout>

    <com.mall.sls.common.widget.scrollview.ReboundScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/title_rel"
        android:layout_marginTop="@dimen/dp_1"
        android:fadingEdge="none"
        android:overScrollMode="never">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout style="@style/back57_ver_ll_style">

                <RelativeLayout
                    android:id="@+id/update_rl"
                    style="@style/height49_rl_style">

                    <com.mall.sls.common.widget.textview.ConventionalTextView
                        style="@style/setting_left_tv_style"
                        android:text="@string/about_app" />

                    <com.mall.sls.common.widget.textview.ConventionalTextView
                        style="@style/setting_right_tv_style"
                        android:text="@string/current_version_info" />

                    <ImageView style="@style/big_black_arrow_back_style" />
                </RelativeLayout>

                <View style="@style/fifteen_back_nine_line_style" />

                <RelativeLayout
                    android:id="@+id/clear_cache_rl"
                    style="@style/height49_rl_style">

                    <com.mall.sls.common.widget.textview.ConventionalTextView
                        style="@style/setting_left_tv_style"
                        android:text="@string/clear_cache" />

                    <ImageView style="@style/big_black_arrow_back_style" />
                </RelativeLayout>

                <View style="@style/fifteen_back_nine_line_style" />

                <RelativeLayout
                    android:id="@+id/contact_service_rl"
                    style="@style/height49_rl_style">

                    <com.mall.sls.common.widget.textview.ConventionalTextView
                        style="@style/setting_left_tv_style"
                        android:text="@string/contact_service" />

                    <ImageView style="@style/big_black_arrow_back_style" />
                </RelativeLayout>
            </LinearLayout>

            <com.mall.sls.common.widget.textview.MediumThickTextView
                android:id="@+id/confirm_bt"
                style="@style/common_next_bt_style"
                android:layout_marginTop="@dimen/dp_320"
                android:text="@string/login_out" />
        </LinearLayout>
    </com.mall.sls.common.widget.scrollview.ReboundScrollView>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:orientation="horizontal">

        <com.mall.sls.common.widget.textview.ConventionalTextView
            android:id="@+id/register_tv"
            style="@style/size15_color_back1_text_style"
            android:paddingLeft="@dimen/dp_5"
            android:paddingTop="@dimen/dp_15"
            android:paddingRight="@dimen/dp_5"
            android:paddingBottom="@dimen/dp_15"
            android:text="@string/registration_agreement" />

        <View
            android:layout_width="@dimen/dp_1"
            android:layout_height="@dimen/dp_22"
            android:layout_gravity="center_vertical"
            android:background="@color/backGround1" />

        <com.mall.sls.common.widget.textview.ConventionalTextView
            android:id="@+id/privacy_tv"
            style="@style/size15_color_back1_text_style"
            android:paddingLeft="@dimen/dp_5"
            android:paddingTop="@dimen/dp_15"
            android:paddingRight="@dimen/dp_5"
            android:paddingBottom="@dimen/dp_15"
            android:text="@string/privacy_policy" />
    </LinearLayout>
</RelativeLayout>
