<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backGround6"
    android:orientation="vertical"
    android:id="@+id/item_rl">

    <RelativeLayout
        android:id="@+id/title_rel"
        style="@style/title_red_rl_style">

        <ImageView
            android:id="@+id/back"
            style="@style/title_left_back_style" />

        <com.mall.sls.common.widget.textview.MediumThickTextView
            android:id="@+id/title"
            style="@style/title_text_style"
            android:text="@string/setting" />
    </RelativeLayout>

    <com.mall.sls.common.widget.scrollview.ReboundScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/title_rel"
        android:fadingEdge="none"
        android:overScrollMode="never">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/feedback_rl"
                style="@style/confirm_order_rl_style"
                android:visibility="gone">

                <com.mall.sls.common.widget.textview.ConventionalTextView
                    style="@style/setting_left_tv_style"
                    android:text="@string/opinions_feedback" />

                <ImageView style="@style/big_black_arrow_back_style" />
            </RelativeLayout>

            <View style="@style/fifteen_back_nine_line_style"
                android:visibility="gone"/>

            <RelativeLayout
                android:id="@+id/update_rl"
                style="@style/confirm_order_rl_style">

                <com.mall.sls.common.widget.textview.ConventionalTextView
                    style="@style/setting_left_tv_style"
                    android:text="@string/version_detection" />

                <ImageView style="@style/big_black_arrow_back_style" />
            </RelativeLayout>

            <View style="@style/fifteen_back_nine_line_style" />

            <RelativeLayout
                android:id="@+id/clear_cache_rl"
                style="@style/confirm_order_rl_style">

                <com.mall.sls.common.widget.textview.ConventionalTextView
                    style="@style/setting_left_tv_style"
                    android:text="@string/clear_cache" />

                <ImageView style="@style/big_black_arrow_back_style" />
            </RelativeLayout>
            <com.mall.sls.common.widget.textview.MediumThickTextView
                android:id="@+id/confirm_bt"
                style="@style/common_next_bt_style"
                android:layout_marginTop="@dimen/dp_320"
                android:text="@string/login_out" />
        </LinearLayout>
    </com.mall.sls.common.widget.scrollview.ReboundScrollView>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true">
        <com.mall.sls.common.widget.textview.ConventionalTextView
            android:id="@+id/register_tv"
            style="@style/size15_color_back1_text_style"
            android:text="@string/registration_agreement"
            android:paddingTop="@dimen/dp_15"
            android:paddingBottom="@dimen/dp_15"
            android:paddingLeft="@dimen/dp_5"
            android:paddingRight="@dimen/dp_5"/>
        <View
            android:layout_width="@dimen/dp_1"
            android:layout_height="@dimen/dp_22"
            android:background="@color/backGround1"
            android:layout_gravity="center_vertical"/>
        <com.mall.sls.common.widget.textview.ConventionalTextView
            android:id="@+id/privacy_tv"
            style="@style/size15_color_back1_text_style"
            android:text="@string/privacy_policy"
            android:paddingTop="@dimen/dp_15"
            android:paddingBottom="@dimen/dp_15"
            android:paddingLeft="@dimen/dp_5"
            android:paddingRight="@dimen/dp_5"/>
    </LinearLayout>
</RelativeLayout>
